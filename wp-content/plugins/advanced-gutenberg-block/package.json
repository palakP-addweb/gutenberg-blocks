{
	"name": "advanced-gutenberg-block",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "blocks/build/index.js",
	"scripts": {
		"start": "wp-scripts start --webpack-src-dir=blocks/src/ --output-path=blocks/build/",
		"build": "wp-scripts build --webpack-src-dir=blocks/src/ --output-path=blocks/build/",
		"start:static": "wp-scripts start --webpack-src-dir=blocks/src/static --output-path=blocks/build/static",
		"build:static": "wp-scripts build --webpack-src-dir=blocks/src/static --output-path=blocks/build/static",
		"start:dynamic": "wp-scripts start --webpack-src-dir=blocks/src/dynamic --output-path=blocks/build/dynamic",
		"build:dynamic": "wp-scripts build --webpack-src-dir=blocks/src/dynamic --output-path=blocks/build/dynamic",
		"start:call-to-action": "wp-scripts start --webpack-src-dir=blocks/src/call-to-action --output-path=blocks/build/call-to-action",
		"build:call-to-action": "wp-scripts build --webpack-src-dir=blocks/src/call-to-action --output-path=blocks/build/call-to-action",
		"start:hero-banner": "wp-scripts start --webpack-src-dir=blocks/src/hero-banner --output-path=blocks/build/hero-banner",
		"build:hero-banner": "wp-scripts build --webpack-src-dir=blocks/src/hero-banner --output-path=blocks/build/hero-banner",
		"start:accordion": "wp-scripts start --webpack-src-dir=blocks/src/accordion --output-path=blocks/build/accordion",
		"build:accordion": "wp-scripts build --webpack-src-dir=blocks/src/accordion --output-path=blocks/build/accordion",
		"start:accordion-item": "wp-scripts start --webpack-src-dir=blocks/src/accordion-item --output-path=blocks/build/accordion-item",
		"build:accordion-item": "wp-scripts build --webpack-src-dir=blocks/src/accordion-item --output-path=blocks/build/accordion-item",
		"start:accordion-group": "wp-scripts start --webpack-src-dir=blocks/src/accordion-group --output-path=blocks/build/accordion-group",
		"build:accordion-group": "wp-scripts build --webpack-src-dir=blocks/src/accordion-group --output-path=blocks/build/accordion-group",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.9.0",
		"tailwindcss": "^3.3.3"
	},
	"dependencies": {
		"bootstrap": "5.3.1",
		"classnames": "^2.3.2",
		"jquery": "^3.7.0",
		"popper.js": "^1.16.1",
		"postcss-nested": "^6.0.1",
		"react-bootstrap": "^2.8.0"
	}
}
